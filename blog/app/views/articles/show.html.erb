<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>
 
<p>
  <strong>Text:</strong>
  <%= @article.text %>
</p>

<% @article.views+=1 %>

<h1><%= link_to 'Like', like_article_path(@article) %> <br>
<%= link_to 'Dis Like', dislike_article_path(@article) %></h1>

<% @article.save %>

<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', articles_path %>
